<div class="container">
        <div class="row">
            <div class="col-md-4 col-md-offset-4">
                <div class="login-panel panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title">Cadastrar</h3>
                    </div>
                    <div class="panel-body">
                        <%= form_for(resource, as: resource_name, url: registration_path(resource_name, role: "form")) do |f| %>
                        <%= devise_error_messages! %>
                            <fieldset>
                                <div class="form-group">
                                    <%= f.email_field :email, autofocus: true, class: "form-control", placeholder: "E-mail", type:"email", autofocus: "autofocus", autocomplete: "email" %>
                                </div>
                                <div class="form-group">
                                    <%= f.password_field :password, autocomplete: "off", class: "form-control", placeholder: "Senha", type:"password" %>
                                </div>
                                <div class="form-group">
                                    <%= f.password_field :password_confirmation, autocomplete: "off", class: "form-control", placeholder: "Confirmar Senha", type:"password" %>
                                </div>
                                <!-- Change this to a button or input when using this as a form -->
                                 <%= f.submit "Logar", class: "btn btn-lg btn-sucess btn-block" %>
                                 <%= render "devise/shared/links" %>
                            </fieldset>
                             <% end %>
                    </div>
                </div>
            </div>
        </div>
    </div>